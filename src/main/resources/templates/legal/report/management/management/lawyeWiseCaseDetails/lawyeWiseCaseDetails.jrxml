	<subDataset name="Table Dataset 1" uuid="f9f32fc4-1539-43d4-a7ea-dc0fccd8c990">
		<parameter name="lawyerName" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select ROW_NUMBER() OVER(ORDER BY CLIENT_ID) AS RowNumber,
BRANCH_NAME,CUSTOMER_ACC_NUM,BORROWER_NAME,
ct.Name as  CaseTypeName,
CASE_NUMBER,co.NAME as courtName,CHEQUE_AMOUNT,SUIT_VALUE
from LITIGATION_CASE_INFO lci
left join LITIGATION_CASE_INFO_LAWYER lcil on lcil.LITIGATION_CASE_INFO_ID= lci.ID
left join LAWYER la on la.ID =lcil.LAWYER_ID
left join CASE_TYPE ct  on ct.ID=lci.CASE_TYPE_ID
left join COURTS co on co.ID = lci.COURT_ID
 where la.NAME = $P{lawyerName}]]>
		</queryString>
		<field name="ROWNUMBER" class="java.math.BigDecimal"/>
		<field name="BRANCH_NAME" class="java.lang.String"/>
		<field name="CUSTOMER_ACC_NUM" class="java.lang.String"/>
		<field name="BORROWER_NAME" class="java.lang.String"/>
		<field name="CASETYPENAME" class="java.lang.String"/>
		<field name="CASE_NUMBER" class="java.lang.String"/>
		<field name="COURTNAME" class="java.lang.String"/>
		<field name="CHEQUE_AMOUNT" class="java.lang.Double"/>
		<field name="SUIT_VALUE" class="java.lang.Double"/>
		<variable name="variable1" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{SUIT_VALUE}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="lawyerName" class="java.lang.String"/>
