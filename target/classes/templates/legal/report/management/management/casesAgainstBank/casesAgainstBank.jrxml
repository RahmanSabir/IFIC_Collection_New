		<parameter name="caseTypeId" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select ROW_NUMBER() OVER(ORDER BY CLIENT_ID) AS RowNumber,
BRANCH_NAME,BORROWER_NAME,CASE_NUMBER,DATE_OF_FILING,
COURT_JURISDICTION,SUIT_VALUE,NEXT_DATE,
ca.NAME as COURSE_OF_ACTION_NAME
from LITIGATION_CASE_INFO lci
left join COURSE_OF_ACTION ca ON ca.ID = lci.COURSE_OF_ACTION_ID
left join CASE_TYPE ct  on ct.ID=lci.CASE_TYPE_ID
 where lci.CASE_TYPE_ID=$P{caseTypeId}]]>
		</queryString>
		<field name="ROWNUMBER" class="java.math.BigDecimal"/>
		<field name="BRANCH_NAME" class="java.lang.String"/>
		<field name="BORROWER_NAME" class="java.lang.String"/>
		<field name="CASE_NUMBER" class="java.lang.String"/>
		<field name="DATE_OF_FILING" class="java.util.Date"/>
		<field name="COURT_JURISDICTION" class="java.lang.String"/>
		<field name="SUIT_VALUE" class="java.lang.Double"/>
		<field name="NEXT_DATE" class="java.util.Date"/>
		<field name="COURSE_OF_ACTION_NAME" class="java.lang.String"/>
		<variable name="variable1" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{SUIT_VALUE}]]></variableExpression>
		</variable>
