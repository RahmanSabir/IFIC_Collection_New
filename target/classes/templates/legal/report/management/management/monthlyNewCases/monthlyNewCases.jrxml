		</parameter>
		<parameter name="startDate" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="endDate" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select ROW_NUMBER() OVER(ORDER BY CLIENT_ID) AS RowNumber,lci.BRANCH_NAME,
lci.CUSTOMER_ACC_NUM,lci.CASE_NUMBER,lci.DATE_OF_FILING,lci.CHEQUE_AMOUNT,lci.SUIT_VALUE,
lci.NEXT_DATE,cs.NAME as Status
from LITIGATION_CASE_INFO lci
left join CASE_STATUS cs on cs.ID= lci.STATUS_ID
 where lci.CASE_TYPE_ID=$P{CASE_TYPE_ID}
 and DATE_OF_FILING between TO_DATE($P{startDate},'yyyy-mm-dd') and TO_DATE($P{endDate},'yyyy-mm-dd')]]>
		</queryString>
		<field name="ROWNUMBER" class="java.math.BigDecimal"/>
		<field name="BRANCH_NAME" class="java.lang.String"/>
		<field name="CUSTOMER_ACC_NUM" class="java.lang.String"/>
		<field name="CASE_NUMBER" class="java.lang.String"/>
		<field name="DATE_OF_FILING" class="oracle.sql.TIMESTAMP"/>
		<field name="CHEQUE_AMOUNT" class="java.lang.Double"/>
		<field name="SUIT_VALUE" class="java.lang.Double"/>
		<field name="NEXT_DATE" class="oracle.sql.TIMESTAMP"/>
		<field name="STATUS" class="java.lang.String"/>
	</subDataset>
