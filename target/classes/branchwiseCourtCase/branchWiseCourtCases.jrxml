	<subDataset name="Table Dataset 1" uuid="bfa6843c-3db4-4f96-a623-5d2f2e1e2933">
		<parameter name="branch_code" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select ROW_NUMBER() OVER(ORDER BY CLIENT_ID) AS RowNumber,DATE_OF_FILING,BORROWER_NAME,LD_NO,CASE_TYPE_SUB_TYPE,CASE_NUMBER,
SUIT_VALUE,slt.PREVIOUSDATE,NEXT_DATE,ca.NAME  as  COURSE_OF_ACTION_NAME
from LITIGATION_CASE_INFO lci
left join COURSE_OF_ACTION ca ON ca.ID = lci.COURSE_OF_ACTION_ID 
left join (select lt.ID ,MAX((SELECT NEXT_DATE  FROM LITIGATION_CASE_INFO_AUD lt WHERE id = lt.ID  ORDER BY REV DESC  OFFSET 1 ROW FETCH FIRST ROW ONLY)) 
AS previousDate from LITIGATION_CASE_INFO_AUD lt group by lt.ID)slt on slt.ID= lci.ID
where lci.BRANCH_NAME=$P{branch_code}]]>
		</queryString>
		<field name="ROWNUMBER" class="java.math.BigDecimal"/>
		<field name="DATE_OF_FILING" class="oracle.sql.TIMESTAMP"/>
		<field name="BORROWER_NAME" class="java.lang.String"/>
		<field name="LD_NO" class="java.math.BigDecimal"/>
		<field name="CASE_TYPE_SUB_TYPE" class="java.lang.String"/>
		<field name="CASE_NUMBER" class="java.lang.String"/>
		<field name="SUIT_VALUE" class="java.lang.Double"/>
		<field name="PREVIOUSDATE" class="oracle.sql.TIMESTAMP"/>
		<field name="NEXT_DATE" class="oracle.sql.TIMESTAMP"/>
		<field name="COURSE_OF_ACTION_NAME" class="java.lang.String"/>
		<variable name="caseAmountTotal" class="java.lang.String" calculation="Sum">
			<variableExpression><![CDATA[$F{SUIT_VALUE}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="branch_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="logo" class="java.lang.Object"/>
	<queryString>
		<![CDATA[select BRANCH_NAME, DATE_OF_FILING,BORROWER_NAME,LD_NO,CASE_TYPE_SUB_TYPE,CASE_NUMBER,LEGAL_EXPENSE,NEXT_DATE_FIXED,NEXT_DATE,COURSE_OF_ACTION_ID 
from LITIGATION_CASE_INFO lci
left join COURSE_OF_ACTION ca ON ca.ID = lci.COURSE_OF_ACTION_ID where lci.BRANCH_NAME=$P{branch_id}]]>
	</queryString>
