		<queryString>
			<![CDATA[select ROW_NUMBER() OVER(ORDER BY CLIENT_ID) AS RowNumber,BRANCH_NAME,CUSTOMER_ACC_NUM,BORROWER_NAME,CASE_NUMBER,
DATE_OF_FILING,DATE_OF_DETECTION,DATE_OF_DETECTION
from LITIGATION_CASE_INFO lci
left join CASE_TYPE ct  on ct.ID=lci.CASE_TYPE_ID
where lci.CASE_TYPE_ID=  $P{CASE_TYPE_ID} and
 DATE_OF_FILING between TO_DATE($P{startDate},'yyyy-mm-dd') and TO_DATE($P{endDate},'yyyy-mm-dd')]]>
		</queryString>
