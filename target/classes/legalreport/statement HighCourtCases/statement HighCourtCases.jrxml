		<queryString>
			<![CDATA[select 
 ROW_NUMBER() OVER(ORDER BY CLIENT_ID) AS RowNumber,
BRANCH_NAME,CUSTOMER_ACC_NUM,BORROWER_NAME,
ct.Name as  CaseTypeName,PETITIONER_NAME,SUBJECT_MATTER_OF_CASE,FIRST_ORDER_DATE,CASE_NUMBER,
DATE_OF_FILING,SUIT_VALUE,CHEQUE_AMOUNT,OUTSTANDING,la.NAME as lawyerName,la.MOBILE_NO,cs.NAME as Status
from LITIGATION_CASE_INFO lci
left join CASE_TYPE ct  on ct.ID=lci.CASE_TYPE_ID
left join LITIGATION_CASE_INFO_LAWYER lcil on lcil.LITIGATION_CASE_INFO_ID= lci.ID
left join LAWYER la on la.ID =lcil.LAWYER_ID
left join CASE_STATUS cs on cs.ID= lci.STATUS_ID
where lci.CASE_TYPE_ID= $P{CASE_TYPE_ID}
and lci.BY_WHOM_FILED = $P{BY_WHOM_FILED}
and CASE_YEAR BETWEEN $P{fromDate} AND $P{endDate}]]>
		</queryString>
